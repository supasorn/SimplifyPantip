{
  "name": "Simplified Pantip",
  "version": "1.0",
  "description": "Make Pantip.com nicer",
  "content_scripts": [
    {
      "matches": [ "http://*/*", "https://*/*", "file://*"],
      "js": ["jquery-1.8.2.min.js", "script.js"]
    }
  ],
  "permissions":  [
    "tabs",
    "http://*/*",
    "https://*/*",
    "file://*"
  ],
  "manifest_version": 2
}
